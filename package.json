{
  "name": "starter-webpack",
  "version": "1.2.0",
  "description": "Webpack starter for Enonic XP",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "watch": "babel --watch=src/main/resources --out-dir=build/resources/main --ignore \"**/*.js\"",
    "build": "npm-run-all build:*",
    "build:js": "babel src/main/resources --out-dir=build/resources/main --ignore \"**/*.js\""
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "autoprefixer": "^9.6.1",
    "browserslist": "^4.6.6",
    "browserslist-config-enonic": "^1.0.4",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^3.0.1",
    "glob": "^7.1.4",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^7.0.1",
    "ramda": "^0.26.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  }
}
